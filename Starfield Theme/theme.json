{
    "name": "Starfield Theme",
    "author": "magesty_",
    "target": "System-Wide",
    "manifest_version": 4,
    "description": "Theme themed after the Starfield game (some elements may differ from the game, as they are either copyrighted or cost money)",
    "version": "v0.1",
    "inject": {
        "colors.css": ["MainMenu"],
        "mainmenu.css": ["MainMenu"]
    },
    "patches": {
        "MainMenu": {
            "type" : "checkbox",
            "default": "Yes",
            "values": {
                "Yes": {},
                "No": {}
            }
        }
    }
}